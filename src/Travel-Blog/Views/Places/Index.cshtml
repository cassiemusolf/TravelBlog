@model IEnumerable<TravelBlog.Models.Place>

<html>
<head>
    <title>Travel Blog</title>
</head>
<body>
    <h2>Travel Blog Places</h2>
    <h4>@Html.DisplayNameFor(model => model.Country)</h4>
    <ul>
        @foreach (var listPlace in Model)
        {
            <li>@Html.ActionLink(@Html.DisplayFor(modelPlace => listPlace.City), "Details", new { id = listPlace.PlaceId })</li>
        }
    </ul>
</body>
</html>